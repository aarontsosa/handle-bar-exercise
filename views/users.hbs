{{#each members}}
    <h1><a href="localhost:3000/users/{{memid}}">{{firstname}} {{surname}}</a></h1>
    <table class="table">
        <tbody>
            <th>
                <th>Address</th>
                <th>Phone Number</th>
            </th>
            <tr>
                <th scope="row"></th>
                    <td>{{address}}</td>
                    <td>{{telephone}}</td>
                </th>
            </tr>
        </tbody>
    </table>
{{/each}}