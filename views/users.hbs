{{#each members}}
    <h1><a href="{{memid}}">{{firstname}} {{surname}}</a></h1>
    <table class="table">
        <tbody>
            <tr>
                <th>Address</th>
                <th>Phone Number</th>
            </tr>
            <tr>
                <td class="col-6">{{address}}</td>
                <td class="col-6">{{telephone}}</td>
            </tr>
        </tbody>
    </table>
{{/each}}